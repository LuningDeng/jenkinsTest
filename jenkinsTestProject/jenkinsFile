pipeline {
    agent any 
    tools { 
        maven 'M3' 
        jdk 'JDK1.8' 
    }
    stages {
        stage ('Initialize') {
            steps {
 					bat 'echo %PATH%'
            }
        }
        stage ('Build') {
            steps {
                bat 'mvn  install' 
            }
        }
    }
}